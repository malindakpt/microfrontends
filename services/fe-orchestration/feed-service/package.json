{
  "name": "feed-service",
  "version": "1.0.0",
  "description": "",
  "main": "dist",
  "typings": "dist",
  "scripts": {
    "clean": "rimraf dist",
    "test:unit": "jest src",
    "test:watch": "yarn run test:unit -- --watch",
    "test": "yarn run lint && yarn run test:unit",
    "start": "yarn run start:prod",
    "watch": "node index.js",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "prettier": "prettier --config prettier.config.js --write 'src/**/*.ts'",
    "build": "tsc --project tsconfig.json",
    "db:reset": "ts-node -r dotenv/config scripts/reset.command.ts",
    "start:dev": "yarn run watch",
    "start:prod": "node -r dotenv/config dist/cli.js",
    "start:debug": "start:dev"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "bin": "./dist/cli.js",
  "license": "MIT",
  "dependencies": {
    "apollo-server-express": "^2.6.9",
    "connect-busboy": "^0.0.2",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^14.4.2",
    "graphql-type-json": "^0.3.0",
    "mime-types": "^2.1.24",
    "pg": "^8.2.1",
    "reflect-metadata": "^0.1.13",
    "response-time": "^2.3.2",
    "tar": "^6.0.1",
    "typeorm": "^0.2.25"
  },
  "devDependencies": {
    "@types/connect-busboy": "^0.0.2",
    "@types/cors": "^2.8.5",
    "@types/express": "^4.17.3",
    "@types/graphql-type-json": "^0.3.0",
    "@types/jest": "^25.1.4",
    "@types/lowdb": "^1.0.9",
    "@types/mime-types": "^2.1.0",
    "@types/node": "^11.13.17",
    "@types/response-time": "^2.3.3",
    "@types/tar": "^4.0.3",
    "jest": "^25.2.4",
    "jest-cli": "^25.2.4",
    "prettier": "^1.19.1",
    "ts-jest": "^25.2.1",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.7.5"
  }
}
